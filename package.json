{
  "name": "clamshell",
  "version": "0.4.1",
  "description": "Blip's mobile companion app for messaging",
  "private": true,
  "repository": "https://github.com/tidepool-org/clamshell.git",
  "author": "Tidepool",
  "scripts": {
    "test": "webpack-dev-server 'mocha!./test/entry.js' --output-file test.js --devtool eval-source-map --cache --colors --progress",
    "start": "webpack-dev-server --devtool eval-source-map --cache --colors --progress --port 3001",
    "build": "npm run build-app && npm run build-config",
    "build-app": "node buildapp",
    "build-config": "node buildconfig",
    "server": "node server",
    "jshint": "gulp jshint",
    "jshint-watch": "gulp jshint-watch"
  },
  "dependencies": {
    "async": "0.9.0",
    "autoprefixer-loader": "0.1.1",
    "bows": "0.5.0",
    "connect": "3.1.0",
    "css-loader": "0.7.0",
    "director": "1.2.3",
    "envify": "1.2.1",
    "envify-loader": "0.1.0",
    "fastclick": "1.0.3",
    "file-loader": "0.6.1",
    "hakken": "0.1.4",
    "imports-loader": "0.6.3",
    "json-loader": "0.5.1",
    "jsx-loader": "0.11.0",
    "less": "1.7.4",
    "less-loader": "0.7.7",
    "lodash": "2.4.1",
    "ms": "0.6.2",
    "react": "0.11.1",
    "serve-static": "1.5.0",
    "shelljs": "0.3.0",
    "style-loader": "0.6.4",
    "sundial": "0.1.6",
    "tidepool-platform-client": "0.6.2",
    "url-loader": "0.5.5",
    "webpack": "1.4.9"
  },
  "devDependencies": {
    "chai": "2.1.1",
    "gulp": "3.8.7",
    "gulp-jshint": "1.9.2",
    "gulp-react": "3.0.0",
    "jshint-stylish": "1.0.1",
    "mocha": "1.21.4",
    "mocha-loader": "0.6.5",
    "script-loader": "0.6.0",
    "sinon-chai": "2.5.0",
    "webpack-dev-server": "1.7.0"
  }
}
